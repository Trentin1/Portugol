Algoritmo "Salario"
Var
   idade : inteiro
   salario : real
   nome : caractere
   cargo : caractere

Inicio
   idade <- 30
   salario <- 5000.00
   nome <- "Lenize O."
   cargo <- "Analista Administrativo"

   escreval("Funcionario: ", nome)
   escreval("Cargo: ", cargo)
   escreval("Idade: ", idade)
   escreval("Salrio: R$", salario)
Fimalgoritmo
