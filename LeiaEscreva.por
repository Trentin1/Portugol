Algoritmo "Write_Print"

Var

   idade1, idade2 : inteiro
   salario1, salario2 : real
   nome1, nome2 : caractere

Inicio

   escreva("Digite o seu nome?")
   leia(nome1)
   escreva("Qual é sua idade?")
   leia(idade1)
   escreva("Qual é o seu salario?")
   leia(salario1)
   escreval()
   escreva("Digite o seu nome?")
   leia(nome2)
   escreva("Qual é sua idade?")
   leia(idade2)
   escreva("Qual é o seu salario?")
   leia(salario2)
   

Fimalgoritmo
