Algoritmo "Salario"
Var
   idade : inteiro
   salario : real
   nome : caractere
   cargo : caractere

Inicio
   idade <- 30
   salario <- 5000.00
   nome <- "Lenize O."
   cargo <- "Analista Administrativo"

   escreval("A funcionária ", nome, " de ", idade, " anos, do Cargo: ", cargo, ", recebe um salário de R$ ", salario,"." )
Fimalgoritmo
