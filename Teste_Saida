Algoritmo "teste_saida"

Var
       a : inteiro
       b : real
       c : caractere
       d : logico

Inicio
         a <- 32
         escreval(a)
         b <- 2.39445
         escreval(b)
         c <- "Bom dia!!"
         escreval(c)
         d <- VERDADEIRO
         escreval(d)


Fimalgoritmo
